{
	"author": "Rob Evans - Irrelon Software Limited",
	"name": "@irrelon/forerunnerdb-core",
	"description": "ForerunnerDB core utilities for operating on JSON data.",
	"version": "3.1.2",
	"private": false,
	"keywords": [
		"irrelon",
		"forerunnerdb",
		"forerunner",
		"database",
		"nosql",
		"mongodb",
		"javascript",
		"document",
		"store",
		"browser",
		"node",
		"data",
		"binding",
		"index",
		"client-side",
		"server-side",
		"lokijs",
		"loki",
		"db",
		"memory",
		"in-memory",
		"indexeddb",
		"localstorage",
		"storage",
		"websql",
		"taffydb",
		"taffy"
	],
	"engines": {
		"node": ">= 10"
	},
	"scripts": {
		"build": "babel src --out-dir dist",
		"eslint": "eslint ./src",
		"eslint-fix": "eslint --fix ./src",
		"test": "NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --require @babel/register --require @babel/core --require @babel/polyfill src/*.test.js src/**/*.test.js src/**/**/*.test.js",
		"test-insert": "NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --require @babel/register --require @babel/core --require @babel/polyfill src/core/operation/insert.test.js",
		"testMon-insert": "nodemon --watch src --exec \"NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --require @babel/register --require @babel/core --require @babel/polyfill src/core/operation/insert.test.js\"",
		"testMon": "nodemon --watch src --exec \"NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --require @babel/register --require @babel/core --require @babel/polyfill src/*.test.js src/**/*.test.js src/**/**/*.test.js\"",
		"testMon-Pipeline": "nodemon --watch src --exec \"NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --require @babel/register --require @babel/core --require @babel/polyfill src/core/Pipeline.test.js\""
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/irrelon/fdb-core.git"
	},
	"main": "./dist/main.js",
	"bugs": {
		"url": "http://github.com/irrelon/fdb-core/issues"
	},
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "^7.9.2",
		"@irrelon/emitter": "^3.0.0",
		"@irrelon/path": "^4.6.4",
		"eslint": "^7.2.0",
		"irrelon-log": "^2.0.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.4",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/polyfill": "^7.8.7",
		"@babel/preset-env": "^7.4.4",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.4.4",
		"@types/assert": "^1.4.6",
		"@types/mocha": "^7.0.2",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.0.5",
		"babel-plugin-const-enum": "^1.0.1",
		"enzyme": "^3.9.0",
		"enzyme-adapter-react-16": "^1.12.1",
		"eslint-config-irrelon": "^1.0.10",
		"express": "^4.16.4",
		"jsdom": "^15.0.0",
		"mocha": "^10.2.0",
		"mocha-parallel-tests": "^2.1.0",
		"nodemon": "^2.0.3",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-test-renderer": "^16.8.6",
		"request": "latest",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.1"
	}
}
